{
	"name": "load_raw_excel_to_demodata",
	"properties": {
		"description": "ATE Conversion Formatting",
		"activities": [
			{
				"name": "Blob to SQL Server",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "src_excel_blob_clean",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@pipeline().parameters.FileName",
								"type": "Expression"
							},
							"SheetName": {
								"value": "@pipeline().parameters.SheetName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AgCore_OnPrem",
						"type": "DatasetReference",
						"parameters": {
							"TableName": {
								"value": "@pipeline().parameters.TableName",
								"type": "Expression"
							},
							"SchemaName": {
								"value": "@pipeline().parameters.SchemaName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"SchemaName": {
				"type": "string",
				"defaultValue": "Clean"
			},
			"TableName": {
				"type": "string",
				"defaultValue": "Sales"
			},
			"FileName": {
				"type": "string",
				"defaultValue": "Customer Deep Dive.xlsx"
			},
			"SheetName": {
				"type": "string",
				"defaultValue": "Sales"
			}
		},
		"folder": {
			"name": "Cleanse"
		},
		"annotations": []
	}
}